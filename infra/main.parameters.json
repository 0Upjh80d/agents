{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "tenantId": {
      "value": "${AZURE_TENANT_ID}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "groupPrincipalId": {
      "value": "${AZURE_GROUP_PRINCIPAL_ID=3f347188-5137-4f5b-af90-c5ffcd231392}"
    },
    "resourceGroupName": {
      "value": "${AZURE_RESOURCE_GROUP_NAME}"
    },
    "aiHubName": {
      "value": "${AZURE_AI_FOUNDRY_HUB_NAME}"
    },
    "aiProjectName": {
      "value": "${AZURE_AI_FOUNDRY_PROJECT_NAME}"
    },
    "keyVaultName": {
      "value": "${AZURE_KEYVAULT_NAME}"
    },
    "storageAccountName": {
      "value": "${AZURE_STORAGE_ACCOUNT_NAME}"
    },
    "openAiServiceName": {
      "value": "${AZURE_OPENAI_SERVICE}"
    },
    "openAiServiceLocation": {
      "value": "${AZURE_OPENAI_SERVICE_LOCATION}"
    },
    "openAiConnectionName": {
      "value": "${AZURE_OPENAI_SERVICE_CONNECTION}"
    },
    "openAiApiVersion": {
      "value": "${AZURE_OPENAI_API_VERSION=2024-10-21}"
    },
    "logAnalyticsName": {
      "value": "${AZURE_LOGANALYTICS_NAME}"
    },
    "applicationInsightsName": {
      "value": "${AZURE_APPLICATIONINSIGHTS_NAME}"
    },
    "containerRegistryName": {
      "value": "${AZURE_CONTAINERREGISTRY_NAME}"
    },
    "useApplicationInsights": {
      "value": "${USE_APPLICATION_INSIGHTS=true}"
    },
    "useContainerRegistry": {
      "value": "${USE_CONTAINER_REGISTRY=false}"
    },
    "deployCosmosDb": {
      "value": "${DEPLOY_COSMOSDB=true}"
    }
  }
}
